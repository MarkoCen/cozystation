{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "module": "ESNext",
    "jsx": "preserve",
    "target": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": false,
    "skipLibCheck": true
  }
}